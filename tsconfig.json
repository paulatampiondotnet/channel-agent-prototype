{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "types": ["google.maps", "grecaptcha", "segment-analytics"],
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "store": ["./src/store"],
      "store/*": ["./src/store/*"],
      "lib/*": ["./src/lib/*"],
      "effects/*": ["./src/effects/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
